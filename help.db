fs:
    -del: fs del file; deletes a file
    -mkdir: fs mkdir folder; creates a folder
    -rmdir: fs rmdir folder
    -run: fs run file (it just call-s it)
    -compile: fs compile
    -decompile: fs decompile

checkup: checks for corruption as text

plugin: aka "pl"
    -list: plugin list;
    -remove: plugin remove name
    -run: plugin run name
    -get: pl get name; downloads plugins from the official Sebix12/OMEGA-II-plugin-repo/main/ repo

crypt:
    -encode: crypt encode name
    -decode: crypt decode name

cls: clears the terminal

exit: exits

kernel:
    -version: kernel version; shows the kernel version
    -help: kernel help; shows this menu; also known as "help"

debug:
    -on
    -off


plugins:  (!Downloadable!)
    backup
    clean-temp
    clear-event-logs      
    clear-printer
    deploy-file-to-systems
    dhcp-watchdog
    disable-hotkey        
    download
    editor
    enable-rdp
    java-runtime-nuker    
    map-printers
    pingup
    repair-windows-update 
    repair_wmi
    setip
    whos-on 